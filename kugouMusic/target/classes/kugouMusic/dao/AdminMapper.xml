<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kugouMusic.dao.AdminDao">
	<select id="getAllAdminInfo" resultType="kugouMusic.entity.Admins">
		select * from admins 
		where AdminName=#{0} and AdminPwd=#{1} 
	</select>
</mapper>